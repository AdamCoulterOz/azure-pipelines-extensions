{
    "manifestVersion": 1,
    "id": "BugChangesetMapper",
    "type": "ms.vss-web.hub",
    "version": "1.0.19",
    "name": "Bug to changeset mapper",
    "description": "Bug to changeset mapper",
    "publisher": "sampathkumar",
    "galleryFlags": [
        "Public",
        "Preview"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "A tool to quickly retrieve changeset(s) associated with a bug.",
    "icons": {
        "default": "img/logo.png"
    },
    "categories": [
        "Plan and track"
    ],
    "tags": [
        "changeset",
        "bug",
        "map"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/sakamati/Extensions/tree/master/BugToChangesetMapper"
    },
    "contributions": [
        {
            "id": "sampathkumar.BugChangesetMapper",
            "type": "ms.vss-web.hub",
            "description": "Gets changeset associated to Bug",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Bug to changeset mapper",
                "order": 99,
                "uri": "Home.html"
            }
        }
    ],
    "files": [
        {
            "path": "Home.html",
            "addressable": true
        },
        {
            "path": "Excel.xslt",
            "addressable": true
        },
        {
            "path": "sdk/scripts",
            "addressable": true
        },
        {
            "path": "img",
            "addressable": true
        }
    ],
    "scopes": [
        "vso.work"
    ]
}
