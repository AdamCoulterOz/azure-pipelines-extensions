      "Tests/**/*.*",
      "coverage/**/*.*"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^13.7.0",
    "@types/q": "1.0.7",
    "azure-pipelines-task-lib": "^2.9.3",
    "generate-mocha": "^1.0.0",
    "minimatch": "^3.0.4",
    "node": "^13.7.0",
    "q": "^1.5.1",
    "sinon": "^8.1.1",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.8",
    "@types/mocha": "^7.0.1",
    "bdd-stdin": "^0.2.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.0",
    "delete": "^1.1.0",
    "generate": "^0.14.0",
    "mocha": "^7.0.1",
    "npm-install-global": "^1.0.0",
    "nyc": "^15.0.0",
    "rewiremock": "^3.13.9",
